{
  "name": "js-dev-env",
  "version": "1.0.0",
  "description": "A Javascript Development Environment to better facilitate creating new projects",
  "private": true,
  "scripts": {
    "prestart": "babel-node buildscripts/startupMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "build": "webpack",
    "open:src": "babel-node buildscripts/srcServer.js",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress ./buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch":"npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NoahDavey/js-dev-env.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NoahDavey/js-dev-env/issues"
  },
  "homepage": "https://github.com/NoahDavey/js-dev-env#readme",
  "dependencies": {
    "chalk": "^4.1.0",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "open": "^7.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.8.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-watch": "^7.0.0",
    "html-webpack-plugin": "^4.4.1",
    "jsdom": "^16.4.0",
    "mocha": "^8.1.3",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-md5-hash": "0.0.6"
  }
}
